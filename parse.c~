#include <stdio.h>
#include "../jsonc.h"

int main() {
  JSON data = json_parse("{\"Array1\":[true,false,null,-2.3,\"Hello world!\"],\"Person\":{\"age\":52.3059,\"occupation\":\"Leaf trimmer\"}}");

  char* stringified = json_stringify(data);
  printf("%s\n", stringified);

  free(stringified);
  json_drop(data);
  return 0;
}
